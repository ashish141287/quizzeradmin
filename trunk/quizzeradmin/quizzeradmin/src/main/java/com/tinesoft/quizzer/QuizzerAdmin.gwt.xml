<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE module SYSTEM
"http://google-web-toolkit.googlecode.com/svn/trunk/distro-source/core/src/gwt-module.dtd">
<module rename-to='quizzeradmin'>
	<!-- Inherit the core Web Toolkit stuff. -->
	<inherits name='com.google.gwt.user.User' />
	
	<!-- Inherit the gilead module . -->
	<inherits name='net.sf.gilead.Gilead4Gwt' />

	<!-- Inherit the mvp4g module . -->
	<inherits name='com.mvp4g.Mvp4gModule' />

	<!-- Inherit the quizzer module . -->
	<inherits name='com.tinesoft.quizzer.QuizzerModule' />

	<!--<inherits name='com.google.gwt.user.theme.standard.Standard' /> -->
	<inherits name='com.google.gwt.user.theme.chrome.Chrome' />
	<!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/> -->

	<entry-point class='com.tinesoft.quizzer.client.QuizzerAdmin' />

	<!-- Specify the paths for translatable code -->
	<source path='client' />
	<source path='shared' />
	

</module>
